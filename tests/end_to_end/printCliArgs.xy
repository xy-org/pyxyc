import libc~[CLib{headers=@{"string.h", "stdio.h"}}] in c;
import xy.sys in sys;
import libxy.string;
import libxy.stdio;
import libxy.memory;

def main~EntryPoint() {
    argc := sys.argc();
    argv := sys.argv();
    for (i in 1:argc) {
        argStr := Str~MemExt {
            addr=argv[i],
            len=c.strlen(argv[i]),
        };
        print(f"{argStr}\n");
    }
}