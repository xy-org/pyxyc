import libc~[Clib{headers=@{"string.h", "stdio.h"}}] in c;
import xy.sys;
import libxy.string;
import libxy.stdio;
import libxy.memory;

func main~EntryPoint() {
    argc := [Global~CmdArgs].argc;
    argv := [Global~CmdArgs].argv;
    for (i in 1:argc) {
        argStr := str (
            addr=argv[i]~Byte,
            size=Usize{c.strlen(argv[i])},
        );
        print(f"{argStr}\n");
    }
}