import libc~[Clib{headers=@{"string.h"}}] in c;

struct Str {
    addr: Ptr;
}

func dtor(s: Str) {
    c.free(s.addr);
}

func createStr1() -> Str {
    return Str{};
}

func createStr2() -> Str {
    return Str{};
}

func copy(s: Str) -> Str {
    return Str{};
}

func startswith(str: Str, prefix: Str) -> Bool {
    return c.strcmp(str.addr, prefix.addr);
}

func test(s1: Str, s2: Str) -> Bool {
    return createStr1()'copy'startswith(s1) || createStr2()'copy'startswith(s2);
}