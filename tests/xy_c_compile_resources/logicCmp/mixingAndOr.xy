func pred1(a: Int) -> Bool {
    return a > 0;
}

func pred2(a: Int) -> Bool {
    return a < -10;
}

func pred3(a: Int, b: Int) -> Bool {
    return a >= b;
}

func test1(a: Int, b: Int) {
    cond: mut = pred1(a);
    cond = cond || pred2(b);
    cond = cond && pred3(a, b);
}

func test2(a: Int, b: Int) -> Bool {
    return (a >= 10 && a < 20) || (b >= a && b < 10);
}