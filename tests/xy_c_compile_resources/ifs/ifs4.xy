# Test if statements with compile-time known consts

func test1(a: Int, b: Int) -> Int {
    if (false) return a + b;
    return a - b;
}

func test2(a: Int, b: Int) -> Int {
    if (true) return a + b;
    return a - b;
    # TODO a double return will be generated
}

func test3(a: Int, b: Int) -> Int {
    c: Int;
    if (false) {
        c = a + b;
    } elif (a > b) {
        c = a;
    } else {
        c = b;
    }
    return c;
}


