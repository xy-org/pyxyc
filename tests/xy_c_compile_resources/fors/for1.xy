func busywait() {
    for (i in :) {
        if (i == 10000) break;
    }
}

func busywait2() {
    for (_ in :10000) {}
}

func sumUpTo(start: Int, end: Int) -> Int {
    return for (i in start:end) -> (res: Int) { res += i; };
}

func multUpTo(start: Int, end: Int, step: Int) -> Int {
    res : mut = 0;
    for (i in start:end:step) res *= sumUpTo(start, i);
    return res;
}

func doubleLoop(limX: Int, limY: Int) -> Int {
    return for (i in :limX) -> (res := 1) {
        for (j in :limY) {
            res *= i + j;
        }
    };
}

func zipLoop(limX: Int, limY: Int) -> Int {
    return for (i in :limX, j in :limY) -> (res := 1) { res *= i + j; };
}
