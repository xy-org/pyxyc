struct ErrorCode {
    code: Int;
}

def to(ec: ErrorCode, :Bool) -> Bool {
    return ec.code != 0;
}

def guards4(a: Int, b: Int) -> (c: Int) || ErrorCode
>> a < b
<< b > c
{
    return 0;
}

def test(a: Int, b: Int) -> Int {
    res : mut = 0;
    res += guards1(a, b);
    res += guards2(a, b);
    res += guards3(res, b);
    res += guards4(a, res);
    res += guards4(
        guards2(guards4(a, b), b),
        guards3(res, res)
    );
    return res;
}