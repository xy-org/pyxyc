;; Test callbacks as members in structs

import xy.ctti;

struct Test~[TagCtor{label="test"}] {}

def test1~Test() {}
def test2~Test() {}

def str~[StrCtor{prefix="", interpolation=false}] (
    addr: Ptr, size: Size
) -> Ptr {
    return addr;
}

struct FuncDesc {
    cb: ()->();
    name: Ptr;
}

def test() {
    tests := @[for (f in $*~Test()) FuncDesc{f, f'nameof}];
}
