struct Array~[elemType: struct] {
    len: Usize,
}

func len(arr: Array) = arr.len;

struct Point {
    x: Float,
    y: Float,
}

func sizeof(:Int) = 4;
func sizeof(:Point) = 8;

func test() -> Usize {
    ints : Array~Int;
    points : Array~Point;
    return ints'len + points'len + ints..elemType'sizeof + points..elemType'sizeof;
}