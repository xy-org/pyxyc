struct Struct {
    val: Int;
}

struct Tag1 {}
struct Tag2 {}
struct DefaultTag {}

func callBasedOnTag(s: Struct) call(s, s..(tag, DefaultTag));

func call(s: Struct, :Tag1) {}
func call(s: Struct, :Tag2) {}
func call(s: Struct, :DefaultTag) {}

func test() {
    s1: Struct~[tag=Tag1];
    s2: Struct~[tag=Tag2];
    s3: Struct;
    callBasedOnTag(s1);
    callBasedOnTag(s2);
    callBasedOnTag(s3);
    callBasedOnTag(s1~void);
    callBasedOnTag(s2~[void]);
    callBasedOnTag(s3~[tag=Tag1]);
    callBasedOnTag(s3~[tag=Tag2]);
}
