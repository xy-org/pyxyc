struct MyInt {
    num: Int;
}

func cmpGt(a: MyInt, b: MyInt) -> Bool {
    return (a - b).num > 0;
}

func cmpLe(a: MyInt, b: MyInt) -> Bool {
    return a.num <= b.num;
}

func add(a: MyInt, b: MyInt) -> MyInt {
    return MyInt{a.num + b.num};
}

func sub(a: MyInt, b: MyInt) -> MyInt {
    return MyInt{a.num - b.num};
}

func mul(a: MyInt, b: MyInt) -> MyInt {
    return MyInt{a.num * b.num};
}

func div(a: MyInt, b: MyInt) -> MyInt {
    return MyInt{a.num / b.num};
}

func cmpMyInts() -> Int {
    a := MyInt{0};
    b := MyInt{1};

    if (a > b) {
        return 1;
    }

    if (a * MyInt{0} > MyInt{1}) {
        return 2;
    }

    if (a / MyInt{1} <= b) {
        return 3;
    }

    return 0;
}


struct Pair {
    a: MyInt;
    b: MyInt;
}

func cmp(p1: MyInt, n: Int) -> Int {
    return p1.num - n;
}

func cmpGt(p1: Pair, p2: Pair) -> Bool {
    return p1.a > p2.a;
}

func cmpPairs() -> Int {
    p1 := Pair{MyInt{0}, MyInt{1}};
    p2 := Pair{MyInt{2}, MyInt{3}};
    if (p1 > p2) {
        return 1;
    }
    return 0;
}