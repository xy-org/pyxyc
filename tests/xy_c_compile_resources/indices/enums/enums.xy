# --- Type Enums ---

struct Status {
    value: Int;

    pending : pseudo = Status{1};
    canceled : pseudo = Status{2};
    processed : pseudo = Status{4};
}

func get(st: Status, test: Status) st.value == test.value;
func set(st: mut Status, val: Status) st.value = test.value;
func cmp(l: Status, r: Status) cmp(l.value, r.value);

func printStatus(st: Status) {
    if (st.pending) {
        # ...
    } elif (st.canceled) {
        # ...
    } elif (st.processed) {
        # ...
    }
}

func testEnums(a: Int) {
    orderStatus := Status.pending;
    printStatus(orderStatus);

    printStatus(Status.processed);

    st : mut = Status.canceled;
    if (a > 0) {
        st = Status.processed;
    }
    printStatus(st);
}
