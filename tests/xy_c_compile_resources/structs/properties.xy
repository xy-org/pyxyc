struct Vec2 {
    x: pseudo = 0;
    y: pseudo = 1;

    xx: pseudo = Pair{0, 0};
    xy: pseudo = Pair{0, 1};
    yx: pseudo = Pair{1, 0};
    yy: pseudo = Pair{1, 1};

    coords: @Float[2];
}

struct Pair {i: Int; j: Int;}

func get(p: Vec2, idx: Int) = p.coords[idx];
func set(p: Vec2, idx: Int, v: Float) p.coords[idx] = v;

func get(p: Vec2, idx: Pair) = Vec2{p[idx.i], p[idx.j]};
func set(p: Vec2, idx: Pair, v: Float) = p.coords[0] = p.coords[0] = v;

func dot(p1: Vec2, p2: Vec2) = p1.x * p2.x + p1.y * p2.y;

func test() {
    p1 : mut = Vec2{.23, .92};
    p2 : mut = Vec2{.4837, .127};
    p3 := p1.yx;
    p1.x = p2.y;
    p2.y = .0;
    mix := (p1.x - p2.x) * (p1.x - p2.x) + (p1.y - p3.y) * (p1.y - p3.y);
}
