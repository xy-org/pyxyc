func func1(x: Int, y: Int) -> Int {
    i : mut = 0;
    while (x < y) {
        x += 10;
        i++;
    }
    return i;
}

func func2(x: Int, y: Int) -> Int {
    return while (x < y) -> (i: Int) {
        x += 10;
        i++;
    };
}

func func3(x: Int, y: Int) -> Int {
    i : mut = 0;
    while (x < y) {
        x += 10;
        i++;
        if (i > 10) break;
    }
    return i;
}

func cond() -> Bool {
    return true;
}

func update() -> Int {
    return 1;
}

func func4(x: Int, y: Int) -> Int {
    return while(cond()) -> (res: Int) { res += update(); };
}
