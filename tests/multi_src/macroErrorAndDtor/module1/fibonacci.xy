struct Fib {
    a := 0,
    b := 1,
}

struct Error {
    num := 0,
}

func to(e: Error, :Bool) e.num != 0;

func dtor(f: Fib) {
    # ...
}

func fibonacci~IterCtor() mkFib();

func mkFib~IterCtor()-> [ Fib ] | Error {
    return Fib{};
}

func valid(fib: Fib) = true;

func next(fib: mut Fib) {
    fib = Fib{fib.a + fib.b, fib.a};
}

func get(fib: Fib) = fib.a;